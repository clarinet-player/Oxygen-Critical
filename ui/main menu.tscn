func _singleplayer():
	Gamemanager.cancel_connect()
	get_tree().change_scene_to_file(\"res://scenes/battleship.tscn\")


func _quit():
	get_tree().quit()


func _set_multiplayer_label(new : String):
	$VBoxContainer/Multiplayer.text = new


func _on_settings_button_down():
	UiManager.set_ui(5)
